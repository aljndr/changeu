<h1>Listing wishes</h1>

<table>
  <tr>
    <th>Regalo</th>
    <th>Costo</th>
    <th>Donde</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wishes.each do |wish| %>
  <tr>
    <td><%= wish.regalo %></td>
    <td><%= wish.costo %></td>
    <td><%= wish.donde %></td>
    <td><%= link_to 'Show', user_wish_path(wish.user_id,wish) %></td>
    <td><%= link_to 'Edit', edit_user_wish_path(wish.user_id,wish) %></td>
    <td><%= link_to 'Destroy', user_wish_path(wish.user_id,wish), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Wish', new_user_wish_path %>
